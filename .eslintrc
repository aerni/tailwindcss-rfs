{
	"extends": ["wesbos"],
	"rules": {
		"no-console": 2,
		"class-methods-use-this": 0,
		"global-require": 0,
		"import/no-extraneous-dependencies": [
			"error",
			{
				"devDependencies": true,
				"optionalDependencies": false,
				"peerDependencies": false,
				"packageDir": "./"
			}
		],
		"prettier/prettier": [
			"error",
			{
				"trailingComma": "es5",
				"singleQuote": true,
				"printWidth": 200,
				"useTabs": false,
				"tabWidth": 4
			}
		]
	}
}
