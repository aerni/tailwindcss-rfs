		"production": "mix --production"
	},
	"repository": "https://github.com/aerni/tailwindcss-rfs",
	"bugs": "https://github.com/aerni/tailwindcss-rfs/issues",
	"keywords": [
		"tailwind",
		"tailwindcss",
		"tailwindcss-plugin",
		"tailwindcss-rfs",
		"plugin",
		"rfs",
		"responsive-font",
		"responsive-font-size"
	],
	"author": "aerni",
	"license": "MIT",
	"dependencies": {
